{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 100,
  "arrowParens": "always",
  "quoteProps": "consistent",
  "bracketSameLine": false,
  "objectWrap": "collapse",
  "experimentalTernaries": true,
  "experimentalOperatorPosition": "start",
  "endOfLine": "lf"
}
